    "eslint-plugin-should-promised": "^2.0.0",
    "mocha": "^5.0.0",
    "moment": "^2.20.1",
    "nyc": "^11.4.1",
    "sinon": "^4.2.0",
    "sinon-chai": "^2.14.0"
